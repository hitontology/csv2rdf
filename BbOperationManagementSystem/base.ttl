@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix hito: <http://hitontology.eu/ontology/>.

hito:Bb rdfs:member hito:BbOperationManagementSystemFunctionCatalogue, hito:BbOperationManagementSystemFeatureCatalogue;
        a hito:Classification;
	rdfs:label "Blue Book classification"@en.
hito:BbOperationManagementSystemFunctionCatalogue a hito:EnterpriseFunctionCatalogue;
	rdfs:comment "Function catalogue defined in tables 6.1 to 6.13 in the Blue Book edition 2"@en;
	rdfs:label "Blue Book Operation Management System Function Catalogue"@en.

hito:BbOperationManagementSystemFeatureCatalogue a hito:FeatureCatalogue;
	rdfs:comment "Feature catalogue defined in tables 6.1 to 6.13 in the Blue Book edition 2"@en;
	rdfs:label "Blue Book Operation Management System Feature Catalogue"@en.
